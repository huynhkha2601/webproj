{{{_sections.footer}}}
<script>
    $(document).ready(function () {
        $(window).scroll(function () {
            if ($(this).scrollTop()) {
                $('#myBtnBackTop').fadeIn();
            } else {
                $('#myBtnBackTop').fadeOut();
            }
        });
        $("#myBtnBackTop").click(function () {
            $('html,body').animate({
                scrollTop: 0
            }, 500);
        });
    });
</script>
