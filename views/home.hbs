<h2 class="product-header d-flex justify-content-center">
    Các sản phẩm gần kết thúc
</h2>
<div id="recentProducts" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#recentProducts" data-slide-to="0" class="active"></li>
        <li data-target="#recentProducts" data-slide-to="1"></li>
        <li data-target="#recentProducts" data-slide-to="2"></li>
        <li data-target="#recentProducts" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <div class="row d-flex align-items-md-center">
                {{#each firstRecent}}
                    <div class="col-md-4 mb-3">
                        <div class="product_card card h-100" style="background-color: unset;">
                            <a style="text-decoration: none" href="">
                                <img src="../public/images/{{productid}}/title/main.jpg" alt="" class="card-img rounded-0">
                            </a>
                            <div class="card-body">
                                <a style="text-decoration: none" href="" target="_blank">
                                    <h4 class="card-text mb-3" style="color:black;">{{productname}}</h4>
                                </a>
                                <h4 class="card-text text-danger">{{formatMoney price}}</h4>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="carousel-item">
            <div class="row d-flex align-items-md-center">
                {{#each secondRecent}}
                    <div class="col-md-4 mb-3">
                        <div class="product_card card h-100" style="background-color: unset;">
                            <a style="text-decoration: none" href="./product_detail.html" target="_blank">
                                <img src="..." alt="" class="card-img rounded-0">
                            </a>
                            <div class="card-body">
                                <a style="text-decoration: none" href="./product_detail.html" target="_blank">
                                    <h4 class="card-text" style="color:black;">Iphone X</h4>
                                </a>
                                <h4 class="card-text text-danger">1,500,000 VND</h4>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="carousel-item">
            <div class="row d-flex align-items-md-center">
                {{#each thirdRecent}}
                    <div class="col-md-4 mb-3">
                        <div class="product_card card h-100" style="background-color: unset;">
                            <a style="text-decoration: none" href="./product_detail.html" target="_blank">
                                <img src="..." alt="" class="card-img rounded-0">
                            </a>
                            <div class="card-body">
                                <a style="text-decoration: none" href="./product_detail.html" target="_blank">
                                    <h4 class="card-text" style="color:black;">Iphone X</h4>
                                </a>
                                <h4 class="card-text text-danger">1,500,000 VND</h4>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="carousel-item">
            <div class="row d-flex align-items-md-center">
                {{#each fourRecent}}
                    <div class="col-md-4 mb-3">
                        <div class="product_card card h-100" style="background-color: unset;">
                            <a style="text-decoration: none" href="./product_detail.html" target="_blank">
                                <img src="..." alt="" class="card-img rounded-0">
                            </a>
                            <div class="card-body">
                                <a style="text-decoration: none" href="./product_detail.html" target="_blank">
                                    <h4 class="card-text" style="color:black;">Iphone X</h4>
                                </a>
                                <h4 class="card-text text-danger">1,500,000 VND</h4>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </div>

    <a class="carousel-control-prev" href="#recentProducts" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#recentProducts" role="button" data-slide="next">
        <i class="fa fa-backward" aria-hidden="true"></i>
        <span class="sr-only">Next</span>
    </a>
</div>

